{:tags []
  :is_new false
  :exports "chart"}

anychart.onDocumentReady(function() {

    //chart type
    chart = anychart.radarChart();

    chart.title().text('Spending');

    chart.line([
        {x: "Administration", value:"22"},
        {x: "Sales", value:"34"},
        {x: "Marketing", value:"16"},
        {x: "Research", value:"12"},
        {x: "Support", value:"38"},
        {x: "Development", value:"47"}
    ]);

    chart.container('container').draw();
});