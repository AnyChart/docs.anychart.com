anychart.onDocumentReady(function() {
var data = new anychart.data.Set([
      ['January', 10000],
      ['February', 12000],
      ['March', 18000],
      ['April', 11000],
      ['May', 9000]
    ]);
chart = anychart.areaChart();
chart.splineArea(data);
chart.yScale().inverted(true);
chart.container('container').draw();
});