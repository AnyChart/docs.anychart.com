{:tags []
     :is_new false
     :exports "chart"}

anychart.onDocumentReady(function() {
  
  // set chart type
  var chart = anychart.column();
  
  // set title text
  var title = chart.title();
  title.text("Sales Performance");
  
  // data
  var data = anychart.data.set([
    ["John", 10000],
    ["Jake", 12000],
    ["Peter", 18000],
    ["James", 11000],
    ["Mary", 9000]
  ]);

  // set data
  chart.column(data);

  // draw chart
  chart.container("container");
  chart.draw();
});