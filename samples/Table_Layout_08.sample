{:tags []
     :is_new false
     :exports "chart"}

anychart.onDocumentReady(function() {

    //create table
    var table = anychart.elements.table();

    //set style
    table.cellBorder(
      'red',  //color
      3,      //thickness
      '5 2'   //dash and gap length
    );

    //set table content
    table.contents([
        [1, 2, 3],
        [4, 5, 6]
    ]);

    //set table container and initiate draw
    table.container('container').draw();
});