{:tags []
     :is_new false
     :exports "chart"}
	 
anychart.onDocumentReady(function() {

    // set stage
    stage = anychart.graphics.create("container");

    //draw the circle
    stage.circle(200, 250, 100).fill(["grey 0.5", "grey 0"])
                .zIndex(1);
   
    //create a square behind the Deathly Hallows symbol
    stage.rect(25, 50, 350, 300);

});