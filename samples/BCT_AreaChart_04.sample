{:tags []
  :is_new false
  :exports "chart"}
anychart.onDocumentReady(function() {

    //data
    var data = anychart.data.set([
        ['January', 10000],
        ['February', 12000],
        ['March', 18000],
        ['April', 11000],
        ['May', 9000]
    ]);

    //chart type
    chart = anychart.areaChart();

    //set title
    chart.title().text('Adjusted Axes Orientation');

    //set axes titles
    chart.xAxis().title().text('Month');
    chart.yAxis().title().text('Sales');

    //adjust axes orientation
    chart.xAxis().orientation('top');
    chart.yAxis().orientation('right');

    //set data
    chart.area(data);

    //draw chart
    chart.container('container').draw();
});