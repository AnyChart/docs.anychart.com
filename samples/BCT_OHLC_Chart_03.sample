{:tags []
     :is_new false
     :exports "chart"}

anychart.onDocumentReady(function() {

    var data = [
        {x: Date.UTC(2007, 7, 28), open:511.53, high:514.98, low:505.79, close:506.40},
        {x: Date.UTC(2007, 7, 29), open:507.84, high:513.30, low:507.23, close:512.88},
        {x: Date.UTC(2007, 7, 30), open:512.36, high:515.40, low:510.58, close:511.40},
        {x: Date.UTC(2007, 7, 31), open:513.10, high:516.50, low:511.47, close:515.25},
        {x: Date.UTC(2007, 8, 4),  open:515.02, high:528.00, low:514.62, close:525.15}
    ];

    //chart type
    chart = anychart.financialChart();

    //chart title
    chart.title().text('Axes Management');


    //adjust x scale type
    chart.xScale()
        .minimum(Date.UTC(2007, 7, 27))
        .maximum(Date.UTC(2007, 8, 5))
        .ticks()
            .interval(0,0,1);

    //det data
    chart.ohlc(data);

    //adjust x and y axes position and title
    chart.xAxis().orientation('top').title().text('Day');
    chart.yAxis().orientation('right').title().text('Price');

    //set x axis lables width for the better outlook
    chart.xAxis().labels().width(60).textFormatter(function(value){
      var date = new Date(value['tickValue']);
      var options = {
        month: 'long',
        day: 'numeric',
      };
      return date.toLocaleDateString('en-US', options);
    });

    //draw
    chart.container('container').draw();
});